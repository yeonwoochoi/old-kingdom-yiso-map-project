      en: 6
  - characterSo: {fileID: 11400000, guid: 9349657a9ece89c429de6b5a8b74c771, type: 2}
    pause: 1
    uiPosition: 0
    message:
      kr: "\uADF8\uB807\uC9C0\uC694. \uC81C\uAC00 \uC880 \uC5B4\uB9AC\uC219\uD569\uB2C8\uB2E4.
